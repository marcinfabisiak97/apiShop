{
    "compilerOptions": {
      "target": "es2017",
      "lib": [
        "dom",
        "dom.iterable",
        "esnext"
      ],
      
      "allowJs": true,
      "skipLibCheck": true,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "noFallthroughCasesInSwitch": true,
      "module": "CommonJS",
      "moduleResolution": "node",
      "noEmit": false,
      "outDir": "build"
    },
    "include": ["index.ts"]
  }